{
  "detection_rules": [
    {
      "name": "CheatEngine_Exact",
      "pattern": "cheatengine.exe",
      "match_type": "EXACT_MATCH",
      "confidence": "CRITICAL",
      "enabled": true,
      "description": "Cheat Engine - Memory editor and debugger",
      "case_sensitive": false,
      "exceptions": [],
      "min_file_size": 0,
      "max_file_size": 0
    },
    {
      "name": "CheatEngine_Variants",
      "pattern": "^cheat.*\\.exe$",
      "match_type": "REGEX_MATCH", 
      "confidence": "HIGH",
      "enabled": true,
      "description": "Cheat Engine variants and renamed versions",
      "case_sensitive": false,
      "exceptions": ["cheater_detector.exe", "cheat_prevention.exe"],
      "min_file_size": 1000000,
      "max_file_size": 50000000
    },
    {
      "name": "OpenKore_Bot",
      "pattern": "openkore.exe",
      "match_type": "EXACT_MATCH",
      "confidence": "CRITICAL",
      "enabled": true,
      "description": "OpenKore - Ragnarok Online bot",
      "case_sensitive": false,
      "exceptions": [],
      "min_file_size": 0,
      "max_file_size": 0
    },
    {
      "name": "WPE_PacketEditor",
      "pattern": "wpepro.exe",
      "match_type": "EXACT_MATCH",
      "confidence": "HIGH",
      "enabled": true,
      "description": "WPE Pro - Packet editor",
      "case_sensitive": false,
      "exceptions": [],
      "min_file_size": 0,
      "max_file_size": 0
    },
    {
      "name": "RPE_PacketEditor", 
      "pattern": "rpe.exe",
      "match_type": "EXACT_MATCH",
      "confidence": "HIGH",
      "enabled": true,
      "description": "RPE - Ragnarok Packet Editor",
      "case_sensitive": false,
      "exceptions": ["helper.exe", "wrapper.exe"],
      "min_file_size": 100000,
      "max_file_size": 0
    },
    {
      "name": "Debuggers_OllyDbg",
      "pattern": "ollydbg.exe",
      "match_type": "EXACT_MATCH",
      "confidence": "MEDIUM",
      "enabled": true,
      "description": "OllyDbg - Assembly level debugger",
      "case_sensitive": false,
      "exceptions": [],
      "min_file_size": 0,
      "max_file_size": 0
    },
    {
      "name": "Debuggers_x64dbg",
      "pattern": "x64dbg.exe",
      "match_type": "EXACT_MATCH",
      "confidence": "MEDIUM",
      "enabled": true,
      "description": "x64dbg - Advanced debugger",
      "case_sensitive": false,
      "exceptions": [],
      "min_file_size": 0,
      "max_file_size": 0
    },
    {
      "name": "IDA_Disassembler",
      "pattern": "^ida(64)?\\.exe$",
      "match_type": "REGEX_MATCH",
      "confidence": "LOW",
      "enabled": true,
      "description": "IDA Pro - Disassembler (could be legitimate)",
      "case_sensitive": false,
      "exceptions": ["nvidia.exe", "aida64.exe"],
      "min_file_size": 5000000,
      "max_file_size": 0
    },
    {
      "name": "Suspicious_Injectors",
      "pattern": ".*inject.*\\.exe$",
      "match_type": "REGEX_MATCH",
      "confidence": "MEDIUM",
      "enabled": true,
      "description": "Potential DLL injectors",
      "case_sensitive": false,
      "exceptions": ["winject_system.exe", "inject_protection.exe"],
      "min_file_size": 10000,
      "max_file_size": 10000000
    },
    {
      "name": "Memory_Scanners",
      "pattern": ".*(scan|memory|hack).*\\.exe$",
      "match_type": "REGEX_MATCH",
      "confidence": "LOW",
      "enabled": false,
      "description": "Potential memory scanners (disabled by default - high false positive)",
      "case_sensitive": false,
      "exceptions": ["memscan_protection.exe", "system_scanner.exe", "antivirus_scanner.exe"],
      "min_file_size": 50000,
      "max_file_size": 0
    }
  ],
  "global_whitelist": [
    "explorer.exe",
    "winlogon.exe", 
    "csrss.exe",
    "dwm.exe",
    "svchost.exe",
    "lsass.exe",
    "services.exe",
    "system",
    "smss.exe",
    "wininit.exe",
    "taskhost.exe",
    "taskhostw.exe",
    "nvidia.exe",
    "aida64.exe",
    "steam.exe",
    "discord.exe",
    "chrome.exe",
    "firefox.exe",
    "notepad.exe",
    "calc.exe"
  ],
  "trusted_paths": [
    "C:\\Windows\\System32\\",
    "C:\\Windows\\SysWOW64\\",
    "C:\\Program Files\\",
    "C:\\Program Files (x86)\\",
    "C:\\Windows\\",
    "C:\\ProgramData\\Microsoft\\",
    "C:\\Users\\%USERNAME%\\AppData\\Local\\Microsoft\\",
    "C:\\Program Files\\NVIDIA Corporation\\",
    "C:\\Program Files (x86)\\Steam\\",
    "C:\\Program Files\\Steam\\"
  ],
  "configuration": {
    "enable_whitelist_protection": true,
    "enable_path_validation": true,
    "enable_file_hash_validation": false,
    "minimum_action_level": "HIGH",
    "log_low_confidence_detections": true,
    "auto_update_rules": false
  }
}
